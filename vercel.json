{
    "version": 2,
    "builds": [
      {
        "src": "weatherapp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb" }
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "(.*)",
        "dest": "weatherapp/wsgi.py"
      }
    ]
  }